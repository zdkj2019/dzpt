<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:clickable="false"
    android:focusable="false"
    android:orientation="vertical" >

    <View
        android:id="@+id/textView2"
        android:layout_width="fill_parent"
        android:layout_height="0.5dip"
        android:background="@android:color/white"
        android:clickable="false"
        android:focusable="false" />
<RelativeLayout
        android:id="@+id/RelaveLout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@drawable/ls_item_log" />

        <TextView
            android:id="@+id/pgdhmy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/imageView1"
            android:text="公司机构"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@android:color/black" />

        <TextView
            android:id="@+id/text_khxl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:text="重庆城二分公司"
            android:textColor="#3D3B3B" />

    </RelativeLayout>
    <RelativeLayout

        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/dddd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="维护厂商"
            android:textColor="#3D3B3B" />

        <TextView
            android:id="@+id/text_pgbm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="30dp"
            android:text="重庆正德"
            android:textColor="#3D3B3B" />

    </RelativeLayout>
    <RelativeLayout

        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item"
        android:gravity="center_vertical" >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="未处理数量"
            android:textColor="#3D3B3B" />

        <TextView
            android:id="@+id/weichuli"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="50dp"
            android:text="0"
            android:textColor="#3D3B3B" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelaveLout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item"
        android:clickable="false"
        android:focusable="false" >

        <TextView
            android:id="@+id/ere"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="处理中的数量"
            android:textColor="#3D3B3B" />

        <TextView
            android:id="@+id/chulizhong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="50dp"
            android:text="0"
            android:textColor="#3D3B3B" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelaveLout3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item" >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="完工数量"
            android:textColor="#3D3B3B" />

        <TextView
            android:id="@+id/wangong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="50dp"
            android:text="0"
            android:textColor="#3D3B3B" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelaveLout4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item" >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="超时数量"
            android:textColor="#3D3B3B" />

        <TextView
            android:id="@+id/chaoshi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="50dp"
            android:text="0"
            android:textColor="#3D3B3B" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelaveLout5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/ls_item" >

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="未超时数量"
            android:textColor="#3D3B3B" />

        <TextView
            android:id="@+id/weichaoshi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="50dp"
            android:text="0"
            android:textColor="#3D3B3B" />
    </RelativeLayout>

</LinearLayout>